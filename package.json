{
  "name": "smk-backend-auth",
  "version": "1.0.0",
  "description": "SMK Authentication Backend with improved error handling",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup-admin": "node scripts/setup-admin.js",
    "migrate": "npx prisma migrate deploy",
    "generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "db:reset": "npx prisma migrate reset --force",
    "postinstall": "npx prisma generate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "joi": "^17.11.0",
    "prisma": "^5.7.1",
    "@prisma/client": "^5.7.1",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "authentication",
    "express",
    "jwt",
    "prisma",
    "postgresql"
  ],
  "author": "SMK Team",
  "license": "MIT"
}
